                <a style="font-size:18px" href="/Surveys/surveyEdit?id=@Model.Id&&num=@Model.num&&phone=@Model.phone&&name=@Model.name">编辑</a>
                }
               
            </h3>
            <div style="overflow-y:auto; overflow-x:auto;font-size:18px;padding:3px">
                @foreach (DataRow item in Model.dt9.Rows)
                {
                    <div class="col-md-6">
                        <div>
                            G1调查对象合作情况:
                            @{
                                int surveyobject = Convert.ToInt32(item[2]);
                                string sur = "";
                                if (surveyobject == 1)
                                {
                                    sur = "很好";
                                }
                                else if (surveyobject == 2)
                                {
                                    sur = "好";
                                }
                                else if (surveyobject == 3)
                                {
                                    sur = "一般";
                                }
                                else if (surveyobject == 4)
                                {
                                    sur = "差";
                                }
                            }
                            @sur
                        </div>
                        <div>
                            G2调查对象回答问题是否可信:
                            @{
                                int surveybelieve = Convert.ToInt32(item[3]);
                                string sub = "";
                                if (surveybelieve == 1)
                                {
                                    sub = "可信";
                                }
                                else if (surveybelieve == 2)
                                {
                                    sub = "尚可信";
                                }
                                else if (surveybelieve == 3)
                                {
                                    sub = "似不可信";
                                }

                            }
                            @sub
                        </div>
                        <div>
                            G3调查结束时间:
                            @{
                                DateTime surveyend = Convert.ToDateTime(item[4]);
                                string sennd = surveyend.ToString("yyyy-MM-dd");
                            }
                            @sennd
                        </div>

                    </div>
                    <div class="col-md-6">
                        <div>
                            G4本调查共用去多少时间:
                            @item[5] 分钟
                        </div>
                        <div>
                            G5整个调查材料质量评价:
                            @{
                                int surveyevaluate = Convert.ToInt32(item[6]);
                                string suva = "";
                                if (surveyevaluate == 1)
                                {
                                    suva = "较高";
                                }
                                else if (surveyevaluate == 2)
                                {
                                    suva = "一般";
                                }
                                else if (surveyevaluate == 3)
                                {
                                    suva = "不太满意";
                                }
                            }
                            @suva
                        </div>

                    </div>
                }
            </div>
        </div>
        <div style="font-size:18px;padding:3px">
            <p>
                <a href="javascript:window.history.go(-1)">返回</a>
            </p>
        </div>
    }
    else
    {

        if (webinsert == "0")
        {
            <h4>暂时没有调查结果</h4>
        }
        else if (webinsert == "1")
        {
            <h4>暂时没有调查结果请新增</h4>
            <p>
                @*@Html.ActionLink("新建", "Create" )*@
                <button type="button" class="btn btn-primary btn-lg" style="font-size: 16px" onclick="window.location.href='/Surveys/Create/?id=@Model.Id&&num=@Model.num&&phone=@Model.phone&&name=@Model.name'">
                    <span class="glyphicon glyphicon-plus"></span>新建调查结果
                </button>
                @*<span class="glyphicon glyphicon-plus"></span>
                    <a href="/UserMangers/Create" style="font-size:22px">新建病人信息</a>*@
            </p>
        }

        <p style="font-size:18px;padding:3px">
            <a href="javascript:window.history.go(-1)">返回</a>
        </p>
    }

}





                    <div class="col-md-2">
                        &nbsp;
                    </div>
                </div>

                int numofsons = Convert.ToInt32(item[2]);

                <div class="form-group" style="font-size:18px;padding:3px;">
                    <label class="control-label col-md-4" style="padding-top:1px;padding-left:0">D1&nbsp;&nbsp;&nbsp;&nbsp;一共有几个儿子(个):</label>
                    <div class="col-md-8" style="margin-bottom:8px">
                        <input class="form-control text-box single-line" data-val="true" id="numofsons" name="numofsons" type="text" value="@numofsons">
                    </div>
                </div>

                int numofdaughters = Convert.ToInt32(item[3]);
